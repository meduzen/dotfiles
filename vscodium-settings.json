        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "todo-tree.tree.labelFormat": "${after}",
    "todo-tree.general.tags": [
        "todo"
    ],
    "todo-tree.regex.regexCaseSensitive": false,
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^[ \\t]*(@*|\\d+.))*\\s*($TAGS)",
    "todo-tree.general.enableFileWatcher": true,
    "todo-tree.filtering.excludeGlobs": [
        "**/node_modules",
        "**/package-lock.json"
    ],
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "git.ignoreMissingGitWarning": true,
    "workbench.colorCustomizations": {
        "[Bearded Theme Coffee Cream]": {
            "editor.hoverHighlightBackground": "#e3dcd9",
            "editor.lineHighlightBackground": "#e3dcd9",
            "editor.lineHighlightBorder": "#e3dcd9",
            "editorGroup.border": "#e3dcd9",
            "editorGroupHeader.border": "#e3dcd9",
            "gitDecoration.modifiedResourceForeground": "#4f68a7",
            "sideBar.border": "#e3dcd9",
            "statusBar.border": "#e3dcd9",
            "tab.activeBorderTop": "#bbaea9",
            "tab.activeForeground": "#706865",
            "tab.border": "#e3dcd9",
            "titleBar.border": "#e3dcd9",
            "titleBar.inactiveBackground": "#e3dcd9",
            "titleBar.inactiveForeground": "#bbaea9"
        }
    },
    "markdown.preview.markEditorSelection": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "svelte.enable-ts-plugin": true,
    "window.title": "${activeEditorShort}${separator}${rootName}${separator}",
    "[ini]": {},
    "files.associations": {
        "*.toml": "ini"
    },
    "workbench.list.defaultFindMode": "filter",
    "workbench.list.horizontalScrolling": true,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.activityBar.location": "hidden",
    "window.commandCenter": false,
    "workbench.iconTheme": "ayu",
    "todo-tree.general.showIconsInsteadOfTagsInStatusBar": true,
    "todo-tree.general.statusBar": "total",
    "todo-tree.highlights.enabled": false,
    "todo-tree.tree.hideTreeWhenEmpty": true,
    "todo-tree.tree.showCurrentScanMode": false,
    "todo-tree.tree.buttons.groupByTag": false,
    "todo-tree.tree.buttons.export": true,
